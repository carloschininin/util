{
  "name": "carloschininin/util",
  "description": "Utilities",
  "version": "0.0.2",
  "license": "proprietary",
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-zip": "*",
    "doctrine/orm": "^2.8",
    "symfony/http-foundation": "^5.1",
    "symfony/string": "^5.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "fzaninotto/faker": "^1.9",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\Util\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\Util\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
