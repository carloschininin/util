{
  "name": "carloschininin/util",
  "type": "library",
  "description": "Utilities",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-zip": "*",
    "ext-mbstring": "*",
    "doctrine/orm": "^2.0|^3.0",
    "lambdish/phunctional": "^2.1",
    "symfony/http-foundation": "^6.0|^7.0",
    "symfony/string": "^6.0|^7.0",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "fzaninotto/faker": "^1.9",
    "phpunit/phpunit": "^10.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\Util\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\Util\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-bcmath": "PHP Extensión de Matemáticas de precisión arbitraria"
  }
}
